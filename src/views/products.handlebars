<h1>Listado Productos</h1>

{{#each products}}
    <li><strong>{{this.title}}</strong><ul><li
        ><strong>Descripcion: </strong>{{this.description}}</li><li
        ><strong>Precio: </strong>{{this.price}}</li>
        <li><strong>Imagen: </strong><img class="imagen-list" src="{{this.thumbnail}}" ></li><li
        >
        <strong>Code: </strong>{{this.code}}</li><li
        ><strong>Stock: </strong>{{this.stock}}</li></ul></li>
  {{/each}}


  <script src="js/products.js"></script>